# codemind-python
n=int(input())
a=0;b=1
for i in range(n-1):
    c=a+b
    a=b
    b=c
    if (c>n):
        if abs(n-c)==abs(n-a):
            print(a,c,end=' ')
            break
        elif abs(n-c)>abs(n-a):
            print(a)
            break
        elif abs(n-c)<abs(n-a):
            print(c)
            break
            
